{
  "rules": {
    "class-name": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "eofline": true,
    "jsdoc-format": true,
    "label-position": true,
    "label-undefined": true,
    "indent": [
      true,
      "spaces"
    ],
    "no-arg": true,
    "no-duplicate-key": true,
    "no-consecutive-blank-lines": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-inferrable-types": true,
    "no-internal-module": true,
    "no-trailing-whitespace": true,
    "no-require-imports": true,
    "no-shadowed-variable": true,
    "no-unreachable": true,
    "no-unused-expression": true,
    "no-unused-variable": [
      true
    ],
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "object-literal-sort-keys": true,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": true,
    "trailing-comma": {
      "multiline": "always"
    },
    "triple-equals": [
      true
    ],
    "typedef-whitespace": [
      true, {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "use-strict": true,
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}
